{
    "extends": ["airbnb", "plugin:node/recommended"],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "func-names": "off",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off",
      "linebreak-style":"off",
      "prefer-destructuring":"off",
      "no-restricted-syntax":"off",
      "lines-between-class-members":"off"
    }
  }
